{
    "name": "vasilgerginski/filament-landing-pages",
    "description": "A Filament plugin for creating and managing landing pages with lead tracking",
    "keywords": [
        "laravel",
        "filament",
        "landing-pages",
        "marketing",
        "lead-generation",
        "livewire"
    ],
    "homepage": "https://github.com/vasilgerginski/filament-landing-pages",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Gerginski",
            "email": "vasil.gerginsky@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2 || ^4.0",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^2.9 || ^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.0 || ^10.0",
        "pestphp/pest": "^2.34 || ^3.0",
        "pestphp/pest-plugin-arch": "^2.7 || ^3.0",
        "pestphp/pest-plugin-laravel": "^2.3 || ^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1 || ^2.0",
        "phpstan/phpstan-phpunit": "^1.3 || ^2.0"
    },
    "suggest": {
        "spatie/laravel-medialibrary": "For advanced media management",
        "artesaos/seotools": "For SEO meta tag management"
    },
    "autoload": {
        "psr-4": {
            "VasilGerginski\\FilamentLandingPages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilGerginski\\FilamentLandingPages\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@clear",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VasilGerginski\\FilamentLandingPages\\FilamentLandingPagesServiceProvider"
            ],
            "aliases": {
                "FilamentLandingPages": "VasilGerginski\\FilamentLandingPages\\Facades\\FilamentLandingPages"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
